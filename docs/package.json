{
  "name": "@stats/docs",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "predev": "bash scripts/build-local-package.sh",
    "dev": "vitepress dev .",
    "prebuild": "bash scripts/build-local-package.sh",
    "build": "npm run docs:api && npm run docs:functions && vitepress build .",
    "preview": "vitepress preview .",
    "docs:api": "typedoc --options typedoc.json",
    "docs:functions": "node scripts/generate-function-pages.js",
    "docs:build": "npm run docs:api && npm run docs:functions && vitepress build docs",
    "docs:serve": "vitepress preview docs",
    "test": "playwright test"
  },
  "dependencies": {
    "@addmaple/stats": "^0.2.4",
    "wasm-feature-detect": "^1.5.0"
  }
}
