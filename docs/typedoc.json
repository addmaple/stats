{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["../js/package/src/index.ts"],
  "tsconfig": "./tsconfig.json",
  "out": "./api",
  "name": "@addmaple/stats API Reference",
  "readme": "none",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "plugin": ["typedoc-plugin-markdown"],
